with "asis";

project PP_Ada_FPops is

   for Source_Dirs use ("src");
   for Object_Dir use "build";
   for Main use ("pp_ada_fpops.adb");
   
   package Compiler is
      for Default_Switches ("ada") use ("-g", "-O2");
   end Compiler;

   package Builder is
      for Default_Switches ("ada") use ("-s", "-x");
   end Builder;

end PP_Ada_FPops;

